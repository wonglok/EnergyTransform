{
  "name": "energy-transform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build; npm run cp;",
    "lint": "vue-cli-service lint",
    "github": "git add .; git commit -am \"github backup\"; git push -u origin master",
    "deploy": "npm run github;",
    "deploy-old": "npm run github; npm run build; npm run cp; npm run netlify;",
    "ssl": "local-ssl-proxy --source 9000 --target 8080",
    "netlify": "netlify deploy --dir=\"./dist\" --prod",
    "cp": "cp _redirects ./dist/_redirects"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^1.0.0",
    "@tweenjs/tween.js": "^17.3.0",
    "core-js": "^2.6.5",
    "firebase": "^5.9.4",
    "imports-loader": "^0.8.0",
    "moment": "^2.24.0",
    "oimo": "^1.0.9",
    "three": "^0.103.0",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "local-ssl-proxy": "^1.3.0",
    "raw-loader": "^2.0.0",
    "vue-template-compiler": "^2.5.21"
  }
}
